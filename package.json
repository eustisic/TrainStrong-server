{
  "name": "server",
  "version": "1.0.0",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest tests/integration",
    "test:integration:mock": "jest --config jest.config.mock.cjs tests/integration",
    "test:unit": "jest tests/unit",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "test:smoke": "tsx tests/test-runner.ts",
    "migrate:up": "tsx src/migrations/run.ts up",
    "migrate:down": "tsx src/migrations/run.ts down",
    "seed": "tsx src/seeds/run.ts",
    "db:setup": "tsx src/scripts/setup-db.ts",
    "db:up": "npm run db:setup && npm run db:migrate:seed",
    "db:migrate:seed": "npm run migrate:up && npm run seed",
    "start:prod": "NODE_ENV=production node dist/index.js",
    "build:prod": "npm run build && npm run db:setup",
    "dev:db": "docker run --name train-strong-db -e POSTGRES_USER=postgres -e POSTGRES_PASSWORD=postgres -e POSTGRES_DB=myapp -p 5432:5432 -d postgres:16-alpine",
    "stop:db": "docker stop train-strong-db && docker rm train-strong-db"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "axios": "^1.12.2",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.0",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "lru-cache": "^11.2.2",
    "morgan": "^1.10.0",
    "pg": "^8.16.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.0.14",
    "@types/pg": "^8.15.4",
    "@types/supertest": "^6.0.3",
    "concurrently": "^9.2.0",
    "jest": "^30.0.5",
    "node-gyp": "^11.4.2",
    "nodemon": "^3.1.10",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.0",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3"
  }
}
